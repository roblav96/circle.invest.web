{
	"name": "circle.invest.web",
	"description": "Circle Invest for Web",
	"version": "0.0.1",
	"scripts": {
		"postinstall": "npm run fix:types",
		"fix:types": "rimraf node_modules/vuex/types/vue.d.ts",
		"purge": "rimraf dist node_modules",
		"client:dev": "vue-cli-service serve --mode development",
		"client:build": "rimraf dist/client && vue-cli-service build --mode production",
		"server:dev": "rimraf dist/server && tsc-watch -p .server.tsconfig.json --onSuccess 'cross-env NODE_ENV=development node dist/server/main'"
	},
